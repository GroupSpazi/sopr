<script>
	$(document).ready(function() {
			$(document).click(function(e) {
				var target = $(e.target);
				if (target.closest('.float-button-btn').length) {
					$('.float-button').toggleClass('active');
				} else {
					$('.float-button').removeClass('active');
				}
			});
	});
</script>
