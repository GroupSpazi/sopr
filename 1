<script>
  var vCard_Preview = {
    CONSTS: {
      currentVcardColor: '#f76707'
    },
    SELECTORS: {
      cardBox: '#vcard-box',
    },
    init: function() {
      var todayIndex = new Date().getDay() - 1;
      if (todayIndex === -1) {
        todayIndex = 6;
      }
      
      $(vCard_Preview.SELECTORS.cardBox).find('.vcard-contact .ripple').css('background-color', vCard_Preview.CONSTS.currentVcardColor);
      $(vCard_Preview.SELECTORS.cardBox).find('.vcard-contact-extras .ripple').css('background-color', vCard_Preview.CONSTS.currentVcardColor);
      $(vCard_Preview.SELECTORS.cardBox).find('.vcard-header .line-separator-50').css('background-color', vCard_Preview.CONSTS.currentVcardColor);
      $(vCard_Preview.SELECTORS.cardBox).find('.line-separator').css('background-color', vCard_Preview.CONSTS.currentVcardColor);
      $(vCard_Preview.SELECTORS.cardBox).find('.vcard-block .vcard-block-title').css('border-color', vCard_Preview.CONSTS.currentVcardColor);
      $(vCard_Preview.SELECTORS.cardBox).find('.vcard-block .opening-hours li').not(".opening-hours__header").eq(todayIndex).addClass('today');
      $(vCard_Preview.SELECTORS.cardBox).find('.vcard-block .opening-hours li.today').css('color', vCard_Preview.CONSTS.currentVcardColor);
      $(vCard_Preview.SELECTORS.cardBox).find('.add-to-contact .btn-add-to-contact').css('background-color', vCard_Preview.CONSTS.currentVcardColor);
      // For minimal theme
      $(vCard_Preview.SELECTORS.cardBox).find('.v-business-hours .business-hours-item').eq(todayIndex).addClass('today');

      // For business
      $(vCard_Preview.SELECTORS.cardBox).find('.vcard-business .vcard-extras-list .extra-item a').css('border-color', vCard_Preview.CONSTS.currentVcardColor);

      // For premium theme
      $(vCard_Preview.SELECTORS.cardBox).find('.vcard-premium .vcard-block').css('border-color', vCard_Preview.CONSTS.currentVcardColor);
      $(vCard_Preview.SELECTORS.cardBox).find('.vcard-premium .vcard-details .vcard-header .description').css('border-color', vCard_Preview.CONSTS.currentVcardColor);
      $(vCard_Preview.SELECTORS.cardBox).find('.vcard-premium .profile-image .img-thumbnail').css('border-color', vCard_Preview.CONSTS.currentVcardColor);

            $('#main-add-to-contact').css('display', 'none');
      
                            }
  };

$(function() {
  vCard_Preview.init();
});
</script>
