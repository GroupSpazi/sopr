        <script type="text/javascript">

          wow = new WOW({
            boxClass:     'wow',      // default
            animateClass: 'animated', // default
            offset:       120,          // default 0
            mobile:       false,       // default
            live:         true        // default
          })
          wow.init();

          document.addEventListener("DOMContentLoaded", () => {
            if (document.querySelector("#home-slider")) {
              width = window.innerWidth;
              var homeSlider = new Siema({
                selector: "#home-slider",
                duration: width > 500 ? 800 : 300,
                easing: "cubic-bezier(.45, 0, .55, 1)",
                perPage: 1,
                startIndex: 0,
                draggable: true,
                multipleDrag: true,
                threshold: 20,
                loop: true,
                rtl: false,
                onChange: function () {
                  // homeSlider.drag.preventClick = true
                  clearInterval(sliderAutoPlay);
                  sliderAutoPlay = setInterval(nextSlideHome, 8000);

                  var dots = document.querySelectorAll(".dots--home .dots__item");

                  dots.forEach((slide, i) => {
                    let addOrRemove = i === this.currentSlide ? "add" : "remove";
                    dots[i].classList[addOrRemove]("dots__item--active");
                  });
                }
              });

              var nextSlideHome = () => {
                homeSlider.next();
              };

              let sliderAutoPlay = setInterval(nextSlideHome, 8000);

              Siema.prototype.dots = function(element) {
                for (let i = 0; i < this.innerElements.length; i++) {
                  var dots = document.querySelector(`.${element}`);
                  var btn = document.createElement("button");
                  var inner = document.createElement('div')
                  btn.appendChild(inner);
                  btn.classList.add("dots__item");
                  btn.addEventListener("click", () => {
                    clearInterval(sliderAutoPlay);
                    sliderAutoPlay = setInterval(nextSlideHome, 8000);
                    this.goTo(i);
                  });

                  dots.appendChild(btn);

                  var dotsArr = Array.prototype.slice.call(dots.querySelectorAll(".dots__item"));
                  dotsArr[0].classList.add("dots__item--active");

                }
              };

              homeSlider.dots("dots--home");

            }

            if (document.querySelector("#industry-slider")) {
              width = window.innerWidth;

              var industrySlider = new Siema({
                selector: "#industry-slider",
                duration: width > 500 ? 800 : 300,
                easing: "cubic-bezier(.45, 0, .55, 1)",
                perPage: 1,
                startIndex: 0,
                draggable: true,
                multipleDrag: true,
                threshold: 20,
                loop: true,
                rtl: false,
                onChange: function () {
                  clearInterval(sliderAutoPlay);
                  // homeSlider.drag.preventClick = true
                  sliderAutoPlay = setInterval(nextSlideIndustry, 8000);

                  var dots = document.querySelectorAll(".dots--industry .dots__item");

                  dots.forEach((slide, i) => {
                    let addOrRemove = i === this.currentSlide ? "add" : "remove";
                    dots[i].classList[addOrRemove]("dots__item--active");
                  });
                }
              });

              var nextSlideIndustry = () => {
                industrySlider.next();
              };

              let sliderAutoPlay = setInterval(nextSlideIndustry, 8000);

              Siema.prototype.dots = function(element) {
                for (let i = 0; i < this.innerElements.length; i++) {
                  var dots = document.querySelector(`.${element}`);
                  var btn = document.createElement("button");
                  var inner = document.createElement('div')
                  btn.appendChild(inner);
                  btn.classList.add("dots__item");

                  btn.addEventListener("click", () => {
                    clearInterval(sliderAutoPlay);
                    sliderAutoPlay = setInterval(nextSlideIndustry, 8000);
                    this.goTo(i);
                  });

                  dots.appendChild(btn);

                  var dotsArr = Array.prototype.slice.call(dots.querySelectorAll(".dots__item"));
                  dotsArr[0].classList.add("dots__item--active");

                }
              };

              industrySlider.dots("dots--industry");

            }

            if (document.querySelector("#industry-slider") || document.querySelector("#home-slider")) {
              document.querySelectorAll(".hero-slider__item").forEach((item) => {
                if (screen.width > 1440) {
                  item.style.backgroundImage = `url(${item.getAttribute("data-large")})`;
                } else if (screen.width <= 1440 && screen.width >= 600) {
                  item.style.backgroundImage = `url(${item.getAttribute("data-medium")})`;
                } else if (screen.width < 600) {
                  item.style.backgroundImage = `url(${item.getAttribute("data-small")})`;
                }
              });
            }
          });

        </script>















